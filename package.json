{
  "name": "platform-service",
  "version": "1.0.0",
  "description": "Node.js service with TypeScript and Fastify",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "fastify",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.2",
    "dotenv": "^16.3.1",
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/client-sqs": "^3.456.0",
    "@aws-sdk/client-redshift-data": "^3.456.0",
    "uuid": "^9.0.1",
    "typedi": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}

